{
  "name": "quark-doc-header",
  "version": "2.0.28",
  "scripts": {
    "dev": "npm run start",
    "start": "vite --open",
    "build": "rimraf lib && vite build --config vite.config.build.ts",
    "prepublishOnly": "npm run build"
  },
  "type": "module",
  "main": "./lib/index.umd.js",
  "module": "./lib/index.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.umd.js",
      "types": "./lib/types/index.d.ts"
    }
  },
  "keywords": [
    "frontend",
    "quarkc",
    "web components"
  ],
  "dependencies": {
    "@docsearch/js": "^3.4.0",
    "quarkc": "^1.0.37"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/node": "^20.2.5",
    "less": "^4.1.3",
    "postcss": "^8.4.24",
    "rimraf": "^5.0.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.0"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hellof2e/quark-doc-header.git"
  }
}
